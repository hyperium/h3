{
    "name": "WebTransport Debug",
    "version": "1.0",
    "description": "Displays a 'Hello World' message",
    "manifest_version": 2,
    "permissions": [
      "webRequest",
      "webNavigation",
      "<all_urls>"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    }
  }
  